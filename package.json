{
	"name": "springroll-keyboard",
	"version": "1.0.1",
	"main": "dist/keyboard.min.js",
	"dependencies": {
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/SpringRoll/Keyboard"
	},
	"scripts": {
		"postinstall": "bower install",
		"concat": "cat src/{USLocale,Keyboard,KeyboardPlugin}.js > dist/keyboard.js",
