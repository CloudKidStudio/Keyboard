		"build": "npm run concat && npm run minify"
	},
	"bugs": {
		"url": "https://github.com/SpringRoll/Keyboard/issues"
	},
	"keywords": [
		"keyboard",
		"input",
		"springroll",
		"educational",
		"cross-browser"
	],
	"license": "MIT",
	"devDependencies": {
		"bower": "^1.8.8",
		"terser": "^4.4.0"
	}
}
